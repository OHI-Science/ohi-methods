	journal = {AMBIO},
	author = {Gonzalez-Mirelis, Genoveva and Lindegarth, Mats and Sköld, Mattias},
	month = mar,
	year = {2014},
	keywords = {Ecology, environmental management, Atmospheric Sciences, Environment, general, Environmental Engineering/Biotechnology, Physical Geography, Marine protected area, Marxan, Systematic conservation planning, VMS data},
	pages = {162--174},
