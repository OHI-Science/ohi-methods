	journal = {Marine Pollution Bulletin},
	author = {de la Torre-Castro, Maricela and Di Carlo, Giuseppe and Jiddawi, Narriman S.},
	month = jun,
	year = {2014},
	keywords = {Seagrass, Marine spatial planning, coastal management, MPA, Seascape, Small-scale fisheries},
