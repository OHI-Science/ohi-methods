	journal = {Marine Policy},
	author = {Jiang, Xu-Zhao and Liu, Tie-Ying and Su, Chi-Wei},
	month = dec,
	year = {2014},
	keywords = {China, Marine economy, Marine policy, Regional development},
	pages = {227--237},
