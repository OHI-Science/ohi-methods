	author = {Mäkinen, Timo and Salmi, Pekka and Forsman, Leena},
	month = apr,
	year = {2014},
	keywords = {Freshwater \& Marine Ecology, Aquaculture, Environmental permit procedure, Fish farming governance, Zoology},
	pages = {711--721},
